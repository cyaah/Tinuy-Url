<!DOCTYPE html>
<html lang="en">
<head>
  <title>TinyApp</title>
</head>
<body>
	<header><% include ./partials/_header.ejs %> </header>
<table>
      <thead>
        <tr>
          <th>Shortened</th>
          <th>Full URL</th>
        </tr>
      </thead>
      <tbody>
        <% for(let X in urls) { %>
        <tr>
          <td> <%= X %> </td>
          <td> <%= urls[X].longurl %> </td>
          
          <td>
          	<form method="POST" action="/urls/<%=X %>/delet">
          		
          		<button>Delete</button>
          	</form>
            
            <form method="POST" action="/urls/<%=X %>">
            <button>Edit</button>
            </form>
          </td>
        </tr>
        <% } %>  
      </tbody>
      </td>
</table>


    <form method="POST" action="/urls/new">

         <a href="http://localhost:8080/urls/new">  Create a new short URL</a>

     </form>
</body>

</html>